<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.palace.bowling_prj_dao.MemberDAO">
	<insert id="userJoin">
		insert into login (userid,userpw,useremail,memname) values (#{userId},#{userPw},#{userEmail},#{memName})
	</insert>
	<select id="loadUser"
		resultType="com.palace.bowling_prj_dto.MemberDTO">
		SELECT * FROM LOGIN WHERE USERID = #{aId}
	</select>

</mapper>